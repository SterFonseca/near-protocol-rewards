{
  "name": "near-protocol-rewards",
  "version": "0.1.2-beta.1",
  "description": "NEAR Protocol Rewards SDK for tracking developer engagement and impact",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "docs",
    "examples"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration:live": "cross-env NODE_ENV=integration jest --config jest.integration.config.js --runInBand",
    "test:unit": "jest --config jest.unit.config.js",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build && npm run test",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "docs": "typedoc --out docs/api src/index.ts"
  },
  "dependencies": {
    "@octokit/rest": "^19.0.0",
    "axios": "^1.6.0",
    "pg": "^8.11.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@types/pg": "^8.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "typedoc": "^0.25.4"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "near",
    "blockchain",
    "rewards",
    "metrics",
    "github",
    "typescript",
    "developer-tools",
    "analytics"
  ],
  "author": "NEAR Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbarnes850/near-protocol-rewards"
  },
  "bugs": {
    "url": "https://github.com/jbarnes850/near-protocol-rewards/issues"
  },
  "homepage": "https://github.com/jbarnes850/near-protocol-rewards#readme"
}
